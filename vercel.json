{
  "version": 2,
  "name": "mimeng-title-generator",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "DEEPSEEK_API_KEY": "@deepseek-api-key"
  },
  "functions": {
    "server.js": {
      "maxDuration": 60
    }
  }
}